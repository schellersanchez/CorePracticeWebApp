@model CorePracticeWebApp.Models.Member
@{
    ViewData["Title"] = "MemberForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MemberForm</h2>
<div class="row">
    @using (Html.BeginForm())
    {
        <div class="col-lg-4">
            <div class="hiddenFields">
                @Html.HiddenFor(m => m.Id)
                @Html.HiddenFor(m => m.IsActive)
            </div>
            <div class="">
                @Html.LabelFor(m => m.FirstName)
                @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.LastName)
                @Html.TextBoxFor(m => m.LastName, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.StreetAddress)
                @Html.TextBoxFor(m => m.StreetAddress, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.City)
                @Html.TextBoxFor(m => m.City, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.State)
                @Html.TextBoxFor(m => m.State, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.Country)
                @Html.TextBoxFor(m => m.Country, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.DateOfBirth)
                @Html.TextBoxFor(m => m.DateOfBirth, new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(m => m.DateOfBap)
                @Html.TextBoxFor(m => m.DateOfBap, new { @class = "form-control" })
            </div>
            
            <button class="btn btn-success">submit</button>

        </div>


    }
</div>
